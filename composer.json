{
  "name": "morozov/kih",
  "description": "RSS generator for Шоу Кремова и Хрусталёва",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-dom": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.2",
    "slim/psr7": "^1.3",
    "slim/slim": "^4.5",
    "ultra-lite/container": "^6.2"
  },
  "require-dev": {
    "doctrine/coding-standard": "^8.2.0",
    "infection/infection": "^0.20",
    "phpunit/phpunit": "^9.4",
    "vimeo/psalm": "^4.2"
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    }
  },
  "autoload": {
    "psr-4": {
      "KiH\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KiH\\Tests\\": "tests/"
    }
  }
}
