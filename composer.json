{
  "name": "morozov/kih",
  "description": "RSS generator for Шоу Кремова и Хрусталёва",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-dom": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^7.3",
    "slim/psr7": "^1.5",
    "slim/slim": "^4.9",
    "ultra-lite/container": "^7.0"
  },
  "require-dev": {
    "doctrine/coding-standard": "^9.0.0",
    "infection/infection": "^0.25",
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^4.10"
  },
  "config": {
    "platform": {
      "php": "8.0.0"
    }
  },
  "autoload": {
    "psr-4": {
      "KiH\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KiH\\Tests\\": "tests/"
    }
  }
}
