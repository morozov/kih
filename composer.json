{
  "name": "morozov/kih",
  "description": "RSS generator for Шоу Кремова и Хрусталёва",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-dom": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.5",
    "slim/psr7": "^1.0",
    "slim/slim": "^4.4",
    "ultra-lite/container": "^6.2"
  },
  "require-dev": {
    "doctrine/coding-standard": "^7.0.2",
    "infection/infection": "^0.15",
    "phan/phan": "^2.6",
    "phpstan/phpstan": "^0.12",
    "phpstan/phpstan-phpunit": "^0.12",
    "phpunit/phpunit": "^9.1",
    "slevomat/coding-standard": "^6.1.5",
    "vimeo/psalm": "^3.10"
  },
  "config": {
    "platform": {
      "php": "7.4.0"
    }
  },
  "autoload": {
    "psr-4": {
      "KiH\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KiH\\Tests\\": "tests/"
    }
  }
}
