{
  "name": "morozov/kih",
  "description": "RSS generator for Шоу Кремова и Хрусталёва",
  "license": "MIT",
  "authors": [
    {
      "name": "Sergei Morozov",
      "email": "morozov@tut.by"
    }
  ],
  "config" : {
    "platform": {
      "php": "7.3.0"
    }
  },
  "require": {
    "php": "^7.3",
    "ext-dom": "*",
    "ext-json": "*",
    "guzzlehttp/guzzle": "^6.3",
    "slim/psr7": "^0.6",
    "slim/slim": "^4.3",
    "ultra-lite/container": "^6.2"
  },
  "require-dev": {
    "doctrine/coding-standard": "^6.0",
    "infection/infection": "^0.13",
    "phan/phan": "^2.2",
    "phpstan/phpstan": "^0.11",
    "phpstan/phpstan-phpunit": "^0.11",
    "phpunit/phpunit": "^8.4",
    "vimeo/psalm": "^3.6"
  },
  "autoload": {
    "psr-4": {
      "KiH\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KiH\\Tests\\": "tests/"
    }
  }
}
